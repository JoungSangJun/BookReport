<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.BookReport"
        tools:targetApi="31">
        <activity
            android:name=".AppGuideActivity"
            android:exported="false" />
        <activity
            android:name=".AppInfoActivity"
            android:exported="false" />
        <activity
            android:name=".PasswordResetActivity"
            android:exported="false" />
        <activity
            android:name="kr.baekseok.register.RegisterActivity"
            android:exported="false" />
        <activity
            android:name="kr.baekseok.login.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".BookAddActivity"
            android:exported="false"
            android:parentActivityName=".BookAddActivity" />
        <activity
            android:name=".ReportAddActivity"
            android:exported="false"
            android:parentActivityName=".ReportAddActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>